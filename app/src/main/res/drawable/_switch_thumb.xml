<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="1dp"
        android:right="1dp"
        android:top="1dp"
        android:bottom="1dp">
        <shape

            android:shape="oval">
            <size
                android:width="20dp"
                android:height="20dp"
                />
            <solid
                android:color="#ffffff"/>
            <stroke
                android:width="1dp"
                android:color="#ffffff"/>


        </shape>
    </item>
</layer-list>